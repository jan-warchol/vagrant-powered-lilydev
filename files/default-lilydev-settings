# -*- mode: ruby -*-
# vi: set ft=ruby :

# This Vagrantfile will be included inside newly created LilyDev box
# (i.e. these settings will be used as the defaults for new LilyDev VMs):

Vagrant.configure(2) do |config|
    config.vm.provider "virtualbox" do |vb|
        vb.gui = true
        vb.cpus = 1
        vb.memory = 768
    end
end
